{
  "name": "jsgui3-html-e2e-tests",
  "version": "1.0.0",
  "description": "E2E test suite for jsgui3-html dev-examples",
  "scripts": {
    "test:e2e": "mocha **/*.test.js --timeout 30000 --exit",
    "test:e2e:counter": "mocha counter.test.js --timeout 30000 --exit",
